   "source": [
    "# P40: Concatenação de DataFrames Incompatíveis"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "     carro     animal\n",
      "0  Hyundai        NaN\n",
      "1  Ranault        NaN\n",
      "2     Fiat        NaN\n",
      "3      NaN   Capivata\n",
      "4      NaN  Bem-te-vi\n"
     ]
    }
   ],
   "source": [
    "import pandas as pd\n",
    "\n",
    "d1 = pd.DataFrame(\n",
    "    {\"carro\": [\"Hyundai\", \"Ranault\", \"Fiat\"]}\n",
    ")\n",
    "d2 = pd.DataFrame(\n",
    "    {\"animal\": [\"Capivata\", \"Bem-te-vi\"]}\n",
    ")\n",
    "\n",
    "d3 = pd.concat([d1, d2], ignore_index=True, sort=False)\n",
    "\n",
    "print(d3)"
   ]
  }
