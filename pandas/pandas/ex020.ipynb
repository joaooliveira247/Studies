    "import pandas as pd\n",
    "\n",
    "dados = {\n",
    "    \"codigo\": [1001, 1002, 1003, 1004, 1005],\n",
    "    \"nome\": [\"Leite\", \"Café\", \"Biscoito\", \"Chá\", \"Torradas\"]\n",
    "}\n",
    "\n",
    "produtos = pd.DataFrame(dados)\n",
    "\n",
    "produtos.to_csv(\"./csv/produtos.csv\", sep=\"\\t\", index=False)"
   ]
  }
