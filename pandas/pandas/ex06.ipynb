    "import pandas as pd\n",
    "\n",
    "alunos = pd.Series(\n",
    "    {\n",
    "        \"M02\": \"Bob\", \"M05\": \"Dayse\", \"M13\": \"Bill\", \"M14\": \"Cris\", \"M19\": \"Jime\"\n",
    "    }\n",
    ")\n",
    "\n",
    "for aluno in alunos:\n",
    "    print(aluno)\n",
    "\n",
    "print(\"-\"*10)\n",
    "\n",
    "for idx in alunos.index:\n",
    "    print(idx)"
   ]
